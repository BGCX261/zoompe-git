<Application x:Class="Zoom.PE.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="CommonPEFileResources.xaml"/>
                <ResourceDictionary Source="Tempates/PEFileModel.xaml"/>
                <ResourceDictionary>
                    <DataTemplate x:Key="System.String">
                        <CheckBox Content="{Binding .}"/>
                    </DataTemplate>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
